<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="SuiteParallel" parallel="tests" thread-count="10" >
    <test name="Authentication android" >
        <parameter name = "platforms" value="android"/>
        <classes>
            <class name="runner.CucumberRunnerTest" />
        </classes>
    </test>
    <test name="Authentication ios" >
        <parameter name = "platforms" value="ios"/>
        <classes>
            <class name="runner.CucumberRunnerTest" />
        </classes>
    </test>
</suite>
